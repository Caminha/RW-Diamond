<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

  <ThingDef ParentName="RockBase">
    <defName>MineableDiamond</defName>
    <label>compacted carbon</label>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(120,130,135)</color>
      <colorTwo>(150,210,240)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1400</MaxHitPoints>
    </statBases>
    <description>Rock containing bits of diamond.</description>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>Diamondrough</mineableThing>
      <mineableYield>5</mineableYield>
      <mineableScatterCommonality>0.065</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>1</min>
        <max>6</max>
      </mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
</ThingDefs>